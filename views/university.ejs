<% title = locals.session.university %>
<%- include('partials/top.ejs') %>
    <main>
        <div class="title">
            <h1>CUNY (cuny.edu) Chats</h1>
            <span>Showing nearby classes happening today</span>
        </div>
        <div class="section">
            <div class="top">
                <h3>Courses</h3>
            </div>
            <table class="chat-list">
                <% for (let i = 0; i < locals.result.length; i++) { let res = result[i]; %> 
                <tr>
                    <td><%= res.serviceName %></td>
                    <td><%= res.subjectName + ' ' + res.courseNumber %></td>
                    <td><%= res.professorName %></td>
                    <td>Section <%= res.section %> (<%= res.daysOfWeek + ' ' + res.startTime %>)</td>
                    <td>
                        <a href="<%= res.link %>"><div class="button">Join</div></a>
                    </td>
                </tr>
                <% } %>
            </table>
        </div>
    </main>
</body>
</html>